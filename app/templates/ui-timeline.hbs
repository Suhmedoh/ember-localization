{{! This file is a modification of html from https://github.com/almasaeed2010/AdminLTE}}

        <!-- Content Header (Page header) -->
        <section class="content-header">
          <h1>
            {{t 'trans.timeline.usertitle' user='Jack'}}
          </h1>
          <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> {{t 'trans.navigation.breadcrumbs.home'}}</a></li>
            <li><a href="#">{{t 'trans.navigation.breadcrumbs.ui'}}</a></li>
            <li class="active">{{t 'trans.navigation.breadcrumbs.timeline'}}</li>
          </ol>
        </section>

        <!-- Main content -->
        <section class="content">

          <!-- row -->
          <div class="row">
            <div class="col-md-12">
              <!-- The time line -->
              <ul class="timeline">
                <!-- timeline time label -->
                <li class="time-label">
                  <span class="bg-red">
                    4 {{t 'trans.time.date.apr'}} 2017
                  </span>
                </li>
                <!-- /.timeline-label -->
                <!-- timeline item -->
                <li>
                  <i class="fa fa-envelope bg-blue"></i>
                  <div class="timeline-item">
                    <span class="time"><i class="fa fa-clock-o"></i> 12:05</span>
                    <h3 class="timeline-header"><a href="#">Support Team</a> {{t 'trans.timeline.user.email'}}</h3>
                    <div class="timeline-body">
                      Etsy doostang zoodles disqus groupon greplin oooj voxy zoodles,
                      weebly ning heekya handango imeem plugg dopplr jibjab, movity
                      jajah plickers sifteo edmodo ifttt zimbra. Babblely odeo kaboodle
                      quora plaxo ideeli hulu weebly balihoo...
                    </div>
                    <div class='timeline-footer'>
                      <a class="btn btn-primary btn-xs">{{t 'trans.timeline.user.readmore'}}</a>
                      <a class="btn btn-danger btn-xs">{{t 'trans.timeline.user.delete'}}</a>
                    </div>
                  </div>
                </li>
                <!-- END timeline item -->
                <!-- timeline item -->
                <li>
                  <i class="fa fa-user bg-aqua"></i>
                  <div class="timeline-item">
                    <span class="time"><i class="fa fa-clock-o"></i> {{t 'trans.time.second' count=35}} {{t 'trans.time.ago'}}</span>
                    <h3 class="timeline-header no-border"><a href="#">Sarah Young</a> {{t 'trans.timeline.user.friend'}}</h3>
                  </div>
                </li>
                <!-- END timeline item -->
                <!-- timeline item -->
                <li>
                  <i class="fa fa-comments bg-yellow"></i>
                  <div class="timeline-item">
                    <span class="time"><i class="fa fa-clock-o"></i> {{t 'trans.time.minute' count=25}} {{t 'trans.time.ago'}}</span>
                    <h3 class="timeline-header"><a href="#">Jay White</a> {{t 'trans.timeline.user.comment'}}</h3>
                    <div class="timeline-body">
                      Take me to your leader!
                      Switzerland is small and neutral!
                      We are more like Germany, ambitious and misunderstood!
                    </div>
                    <div class='timeline-footer'>
                      <a class="btn btn-warning btn-flat btn-xs">{{t 'trans.timeline.user.view'}}</a>
                    </div>
                  </div>
                </li>
                <!-- END timeline item -->
                <!-- timeline time label -->
                <li class="time-label">
                  <span class="bg-green">
                    3 {{t 'trans.time.date.jan'}} 2014
                  </span>
                </li>
                <!-- /.timeline-label -->
                <!-- timeline item -->
                <li>
                  <i class="fa fa-camera bg-purple"></i>
                  <div class="timeline-item">
                    <span class="time"><i class="fa fa-clock-o"></i> {{t 'trans.time.day.day' count=2}} {{t 'trans.time.ago'}}</span>
                    <h3 class="timeline-header"><a href="#">Mina Lee</a> {{t 'trans.timeline.user.photo' count=4}}</h3>
                    <div class="timeline-body">
                      <img src="http://placehold.it/150x100" alt="..." class='margin' />
                      <img src="http://placehold.it/150x100" alt="..." class='margin'/>
                      <img src="http://placehold.it/150x100" alt="..." class='margin'/>
                      <img src="http://placehold.it/150x100" alt="..." class='margin'/>
                    </div>
                  </div>
                </li>
                <!-- END timeline item -->
                <!-- timeline item -->
                <li>
                  <i class="fa fa-video-camera bg-maroon"></i>
                  <div class="timeline-item">
                    <span class="time"><i class="fa fa-clock-o"></i> {{t 'trans.time.day.day' count=5}} {{t 'trans.time.ago'}}</span>
                    <h3 class="timeline-header"><a href="#">Mr. Doe</a> {{t 'trans.timeline.user.video'}}</h3>
                    <div class="timeline-body">
                      <div class="embed-responsive embed-responsive-16by9">
                        <iframe class="embed-responsive-item" src="https://www.youtube.com/embed/tMWkeBIohBs" frameborder="0" allowfullscreen></iframe>
                      </div>
                    </div>
                    <div class="timeline-footer">
                      <a href="#" class="btn btn-xs bg-maroon">{{t 'trans.timeline.user.viewall'}}</a>
                    </div>
                  </div>
                </li>
                <!-- END timeline item -->
                <li>
                  <i class="fa fa-clock-o bg-gray"></i>
                </li>
              </ul>
            </div><!-- /.col -->
          </div><!-- /.row -->



        </section><!-- /.content -->
